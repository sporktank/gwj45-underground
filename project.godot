; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Lizard",
"language": "GDScript",
"path": "res://lizard.gd"
} ]
_global_script_class_icons={
"Lizard": ""
}

[application]

config/name="Underground"
run/main_scene="res://game.tscn"
boot_splash/image="res://common/black.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icons/desktop-icon.ico"

[autoload]

Enums="*res://common/enums.gd"
Events="*res://common/events.gd"
Scenes="*res://common/scenes.gd"
Global="*res://common/global.gd"

[display]

window/size/width=960
window/size/height=540
window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[importer_defaults]

texture={
